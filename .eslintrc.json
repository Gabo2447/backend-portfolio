{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["standard"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "no-extra-semi": 1,
    "quotes": ["error", "single"],
    "no-unused-vars": ["warn"],
    "no-console": "off"
  }
}